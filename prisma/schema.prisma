datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id        String   @id @default(uuid())
  title     String
  slug      String   @unique
  summary   String?
  content   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
